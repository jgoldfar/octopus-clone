# -*- coding: utf-8 mode: shell-script -*-

Test       : Tkatchenko-Scheffler van der Waals correction (self-consistent) in solid 
Program    : octopus
TestGroups : functionals, long-run, VDW_test
Enabled    : Yes

Processors : 4
Input      : 12-vdw_solid_c6.01-gs_diamond.inp 

Precision: 0.0001
match ; van der Waals energy ; GREPFIELD(static/info, 'vanderWaals =', 3) ; -0.01489468
Precision: 0.001
match ; Total energy       ; GREPFIELD(static/info, 'Total       =', 3) ;  -11.43508804
match ; Eigenvalues sum    ; GREPFIELD(static/info, 'Eigenvalues =', 3) ; -0.76762164
Precision: 0.0002
match ; Hartree energy     ; GREPFIELD(static/info, 'Hartree     =', 3) ; 0.95851651
Precision: 0.0001
match ; Ion-ion energy     ; GREPFIELD(static/info, 'Ion-ion     =', 3) ; -10.73490075
match ; Int[n*v_xc]        ; GREPFIELD(static/info, 'Int\[n\*v_xc\] =', 3) ; -4.59170850
match ; Exchange energy    ; GREPFIELD(static/info, 'Exchange    =', 3) ; -3.19143751
match ; Correlation energy ; GREPFIELD(static/info, 'Correlation =', 3) ; -0.35942545
match ; Kinetic energy     ; GREPFIELD(static/info, 'Kinetic     =', 3) ; 8.45346084
match ; External energy    ; GREPFIELD(static/info, 'External    =', 3) ; -6.54531633

Precision: 0.0001
match ; Force C1 (x)   ; GREPFIELD(static/info, '1         C', 3) ; 0.000258
match ; Force C1 (y)   ; GREPFIELD(static/info, '1         C', 4) ; -0.000437
match ; Force C1 (z)   ; GREPFIELD(static/info, '1         C', 5) ; -0.000072


match ; Force C2 (x)   ; GREPFIELD(static/info, '2         C', 3) ; -0.000745 
match ; Force C2 (y)   ; GREPFIELD(static/info, '2         C', 4) ; -0.000050
match ; Force C2 (z)   ; GREPFIELD(static/info, '2         C', 5) ; -0.000415


Precision: 0.001
match ; C6 eff  C1-C1   ; GREPFIELD(static/c6ab_eff, '1        1', 3) ; 3.89368557E+01
match ; C6 eff  C1-C2   ; GREPFIELD(static/c6ab_eff, '1        2', 3) ; 3.89311085E+01
match ; C6 eff  C2-C1   ; GREPFIELD(static/c6ab_eff, '2        1', 3) ; 3.89311085E+01
match ; C6 eff  C2-C2   ; GREPFIELD(static/c6ab_eff, '2        2', 3) ; 3.89253622E+01
#Input      : 12-vdw_solid_c6.02-gs_graphen.inp
