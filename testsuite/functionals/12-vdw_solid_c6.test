match ; External energy    ; GREPFIELD(static/info, 'External    =', 3) ;  14.48505570
match ; van der Waals energy ; GREPFIELD(static/info, 'vanderWaals =', 3) ; -0.00335713

match ; Force C1 (x)   ; GREPFIELD(static/info, '1         C', 3) ; -0.000796
