# -*- coding: utf-8 mode: shell-script -*-

Test       : Tkatchenko-Scheffler van der Waals correction (self-consistent) in solid 
Program    : octopus
TestGroups : functionals, long-run, VDW_test
Enabled    : Yes

Processors : 3
Input      : 12-vdw_solid_c6.01-gs_diamond.inp 

match ; Total energy       ; GREPFIELD(static/info, 'Total       =', 3) ; -11.32044297
match ; Eigenvalues sum    ; GREPFIELD(static/info, 'Eigenvalues =', 3) ; -0.55730551
match ; Hartree energy     ; GREPFIELD(static/info, 'Hartree     =', 3) ;  1.05336102
match ; Ion-ion energy     ; GREPFIELD(static/info, 'Ion-ion     =', 3) ; -10.73490075
match ; Exchange energy    ; GREPFIELD(static/info, 'Exchange    =', 3) ; -3.23177519
match ; Correlation energy ; GREPFIELD(static/info, 'Correlation =', 3) ; -0.35466610
match ; Kinetic energy     ; GREPFIELD(static/info, 'Kinetic     =', 3) ;  8.98735287
match ; External energy    ; GREPFIELD(static/info, 'External    =', 3) ; -7.02336815
match ; van der Waals energy ; GREPFIELD(static/info, 'vanderWaals =', 3) ; -0.01443613

match ; Force C1 (x)   ; GREPFIELD(static/info, '1         C', 3) ; -0.000830
match ; Force C1 (y)   ; GREPFIELD(static/info, '1         C', 4) ; -0.000830
match ; Force C1 (z)   ; GREPFIELD(static/info, '1         C', 5) ; -0.000923
match ; Force C2 (x)   ; GREPFIELD(static/info, '2         C', 3) ;  0.000370
match ; Force C2 (y)   ; GREPFIELD(static/info, '2         C', 4) ;  0.000370
match ; Force C2 (z)   ; GREPFIELD(static/info, '2         C', 5) ;  0.000464

match ; C6 eff  C1-C1   ; GREPFIELD(static/c6ab_eff, '1        1', 3) ; 3.70754587E+01
match ; C6 eff  C1-C2   ; GREPFIELD(static/c6ab_eff, '1        2', 3) ; 3.70687533E+01
match ; C6 eff  C2-C1   ; GREPFIELD(static/c6ab_eff, '2        1', 3) ; 3.70687533E+01
match ; C6 eff  C2-C2   ; GREPFIELD(static/c6ab_eff, '2        2', 3) ; 3.70620491E+01


Processors : 2
Input      : 12-vdw_solid_c6.02-gs_graphen.inp

