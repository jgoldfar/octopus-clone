# -*- coding: utf-8 mode: shell-script -*-
# $Id: $

Test       : Polarizable Continuum Model (PCM)
Program    : octopus
TestGroups : short-run, finite_systems_3d
Enabled    : No
Processors : serial

Input      : 29-pcm_chlorine_anion.01-ground_state.inp
Precision: 0.0001
match ; eigenvalue [1]                ;  GREPFIELD(static/info, '1   --', 3)            ; -16.872746
match ; eigenvalue [2]                ;  GREPFIELD(static/info, '2   --', 3)            ;  -5.298475
match ; eigenvalue [3]                ;  GREPFIELD(static/info, '3   --', 3)            ;  -5.298174 
match ; eigenvalue [4]                ;  GREPFIELD(static/info, '4   --', 3)            ;  -5.296101
match ; electrons-solvent int. energy ;  GREPFIELD(static/info, 'E_e-solvent =', 3)     ; -25.17740165
match ; nuclei-solvent int. energy    ;  GREPFIELD(static/info, 'E_n-solvent =', 3)     ;  22.12428454
match ; molecule-solvent int. energy  ;  GREPFIELD(static/info, 'E_M-solvent =', 3)     ;  -3.05311711
match ; electronic pol. charge        ;  GREPFIELD(pcm/pcm_info.out, '       7   ', 7)     ;  -7.96505420
match ; nuclear pol. charge           ;  GREPFIELD(pcm/pcm_info.out, '       7   ', 8)     ;   6.99925894


Input      : 29-pcm_chlorine_anion.02-td_prop.inp
Precision: 0.0001
match ; M-solvent int. energy @ t=0    ;  GREPFIELD(td.general/energy,'       0', 12)   ; -3.052442810227e+00
match ; M-solvent int. energy @ t=5*dt ;  GREPFIELD(td.general/energy,'       5', 12)   ; -3.052443061462e+00

