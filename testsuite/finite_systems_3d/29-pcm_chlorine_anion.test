Test       : Polarizable Continuum Model (PCM)
Program    : octopus
TestGroups : short-run, finite_systems_3d
Enabled    : Yes
Processors : 4

Input      : 29-pcm_chlorine_anion.01-ground_state.inp
match ; eigenvalue [1]                ;  GREPFIELD(static/info, '1   --', 3)            ; -17.161694; 0.0001
match ; eigenvalue [2]                ;  GREPFIELD(static/info, '2   --', 3)            ;  -5.579994; 0.0001
match ; eigenvalue [3]                ;  GREPFIELD(static/info, '3   --', 3)            ;  -5.575194 ; 0.0001
match ; eigenvalue [4]                ;  GREPFIELD(static/info, '4   --', 3)            ;  -5.574328; 0.0001
match ; electrons-solvent int. energy ;  GREPFIELD(static/info, 'E_e-solvent =', 3)     ; -27.07646806; 0.0001
match ; nuclei-solvent int. energy    ;  GREPFIELD(static/info, 'E_n-solvent =', 3)     ;  23.86066947; 0.0001
match ; molecule-solvent int. energy  ;  GREPFIELD(static/info, 'E_M-solvent =', 3)     ;  -3.21579859; 0.0001
match ; electronic pol. charge        ;  GREPFIELD(pcm/pcm_info.out, '       9   ', 7)  ;  -7.94244818; 0.0001
match ; nuclear pol. charge           ;  GREPFIELD(pcm/pcm_info.out, '       9   ', 9)  ;   6.99925894; 0.0001


Input      : 29-pcm_chlorine_anion.02-td_prop.inp
match ; M-solvent int. energy @ t=0    ;  GREPFIELD(td.general/energy,'       0', 12)   ; -3.215791814616e+00; 0.0001
match ; M-solvent int. energy @ t=5*dt ;  GREPFIELD(td.general/energy,'       5', 12)   ; -3.215791811556e+00; 0.0001

