# -*- coding: utf-8 mode: shell-script -*-
# $Id$

Test       : Static magnetic field
Program    : octopus
TestGroups : long-run, finite_systems_3d
Enabled    : Yes

Input: 03-magnetic.01-gs-unpolarized.inp
match ; Eigenvalue  1    ; GREPFIELD(static/info, '  1   --', 3) ; -0.248426; 0.0001
match ; Eigenvalue  2    ; GREPFIELD(static/info, '  2   --', 3) ; -0.056939; 0.0001
match ; Eigenvalue  3    ; GREPFIELD(static/info, '  3   --', 3) ; -0.048611; 0.0001
match ; Eigenvalue  4    ; GREPFIELD(static/info, '  4   --', 3) ; -0.039065; 0.0001
match ; Total energy     ; GREPFIELD(static/info, 'Total       =', 3) ; -1.91221164; 0.0001
match ; Free energy      ; GREPFIELD(static/info, 'Free        =', 3) ; -1.93735493; 0.0001
match ; Fermi energy     ; GREPFIELD(static/info, 'Fermi energy =', 4); -0.066015; 0.0001

