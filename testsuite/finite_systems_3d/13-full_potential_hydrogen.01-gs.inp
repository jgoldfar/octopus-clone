# -*- coding: utf-8 mode: shell-script -*-

%Coordinates
 'H_f' | 0 | 0 | 0
%
%Species 
 'H_f' | species_full_gaussian
%

Radius = 8.0
Spacing = 0.4
fromScratch = yes

TheoryLevel = independent_particles

EigenSolverMaxIter = 300

ExtraStates = 1
%Occupations
0 | 1
%

LCAOComplexYlms = true
%StaticMagneticField
0.0 | 0.0 | 0.5
%

CalculateSelfInducedMagneticField = yes

Output = matrix_elements + potential
OutputFormat = axis_y
OutputMatrixElements = momentum + ang_momentum
# + one_body + two_body + ks_multipoles

FilterPotentials = filter_none

