Test       : Multiresolution
Program    : octopus
TestGroups : long-run, finite_systems_3d
Enabled    : Yes

# incredibly, all buildslaves give exactly the same values here.

Processors: 1
# segfaults on two processors
Input: 25-multiresolution.01-Hatom.inp
match ; Total energy                  ; GREPFIELD(static/info, 'Total       =', 3) ; -0.50011825; 1e-06
match ; Species displacement          ; GREPFIELD(out, 'H_f atom displaced', 7)    ;  0.0; 1e-06
match ; Eigenvalue                    ; GREPFIELD(static/info, ' 1   -- ', 3)      ; -0.500118; 1e-06
match ; Occupation                    ; GREPFIELD(static/info, ' 1   -- ', 4)      ;  1.0; 1e-06
