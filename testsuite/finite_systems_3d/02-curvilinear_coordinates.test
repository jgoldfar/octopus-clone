# -*- coding: utf-8 mode: shell-script -*-
# $Id$

Test       : Curvilinear Coordinates
Program    : octopus
TestGroups : short-run, finite_systems_3d
Enabled    : Yes

Processors : 1
# curvilinear not compatible with domain parallelization
# there is only one state, so we cannot use state parallelization
Input: 02-curvilinear_coordinates.01-gygi.inp

match ; Total energy       ; GREPFIELD(static/info, 'Total       =', 3) ; -2.86805481; 0.0001
match ; Ion-ion energy     ; GREPFIELD(static/info, 'Ion-ion     =', 3) ; 0.00000000; 0.0001
match ; Eigenvalues sum    ; GREPFIELD(static/info, 'Eigenvalues =', 3) ; -1.15215173; 0.0001
match ; Hartree energy     ; GREPFIELD(static/info, 'Hartree     =', 3) ; 2.02132612; 0.0001
match ; Int[n*v_xc]        ; GREPFIELD(static/info, 'Int\[n\*v_xc\] =', 3) ; -1.28990262; 0.0001
match ; Exchange energy    ; GREPFIELD(static/info, 'Exchange    =', 3) ; -0.87307978; 0.0001
match ; Correlation energy ; GREPFIELD(static/info, 'Correlation =', 3) ; -0.11143832; 0.0001
match ; Kinetic energy     ; GREPFIELD(static/info, 'Kinetic     =', 3) ; 2.81804915; 0.0001
match ; External energy    ; GREPFIELD(static/info, 'External    =', 3) ; -6.72295049; 0.0001

match ; Eigenvalue         ; GREPFIELD(static/info, '1   --', 3) ; -0.576076; 0.0001
