# -*- coding: utf-8 mode: shell-script -*-
# $Id$

Test       : Silicon force
Program    : octopus
TestGroups : short-run, periodic_systems
Enabled    : Yes

Input      : 11-silicon_force.01-gs.inp

match ; Total k-points   ; GREPFIELD(static/info, 'Total number of k-points', 6) ; 8
match ; Reduced k-points ; GREPFIELD(static/info, 'Number of symmetry-reduced k-points', 6) ; 8
match ; Space group        ; GREPFIELD(out, 'Space group', 4) ; 1
match ; No. of symmetries  ; GREPFIELD(out, 'symmetries that can be used', 5)  ;  1

match ; Total energy       ; GREPFIELD(static/info, 'Total       =', 3) ; -34.20844087
match ; Ion-ion energy     ; GREPFIELD(static/info, 'Ion-ion     =', 3) ; -33.57875943
match ; Eigenvalues sum    ; GREPFIELD(static/info, 'Eigenvalues =', 3) ;  -1.40966013
match ; Hartree energy     ; GREPFIELD(static/info, 'Hartree     =', 3) ;   2.12581349
match ; Exchange energy    ; GREPFIELD(static/info, 'Exchange    =', 3) ;  -8.06662633
match ; Correlation energy ; GREPFIELD(static/info, 'Correlation =', 3) ;  -1.49536144
match ; Kinetic energy     ; GREPFIELD(static/info, 'Kinetic     =', 3) ;  12.67431751
match ; External energy    ; GREPFIELD(static/info, 'External    =', 3) ;  -5.94295164

match ; k-point 1 (x) ; GREPFIELD(static/info, '#k =   1', 7) ; 0.25
match ; k-point 1 (y) ; GREPFIELD(static/info, '#k =   1', 8) ; 0.25
match ; k-point 1 (z) ; GREPFIELD(static/info, '#k =   1', 9) ; 0.25
