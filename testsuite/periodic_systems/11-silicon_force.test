# -*- coding: utf-8 mode: shell-script -*-

Test       : Silicon force
Program    : octopus
TestGroups : short-run, periodic_systems
Enabled    : Yes

Input      : 11-silicon_force.01-gs.inp

match ; Total k-points   ; GREPFIELD(static/info, 'Total number of k-points', 6) ; 8
match ; Reduced k-points ; GREPFIELD(static/info, 'Number of symmetry-reduced k-points', 6) ; 4
match ; Space group        ; GREPFIELD(out, 'Space group', 4) ; 1
match ; No. of symmetries  ; GREPFIELD(out, 'symmetries that can be used', 5)  ;  1

match ; Total energy       ; GREPFIELD(static/info, 'Total       =', 3) ; -32.00258354
match ; Ion-ion energy     ; GREPFIELD(static/info, 'Ion-ion     =', 3) ; -31.21044509
match ; Eigenvalues sum    ; GREPFIELD(static/info, 'Eigenvalues =', 3) ;  -1.28306009
match ; Hartree energy     ; GREPFIELD(static/info, 'Hartree     =', 3) ;   2.45455249
match ; Exchange energy    ; GREPFIELD(static/info, 'Exchange    =', 3) ;  -8.18493460
match ; Correlation energy ; GREPFIELD(static/info, 'Correlation =', 3) ;  -1.50329455
match ; Kinetic energy     ; GREPFIELD(static/info, 'Kinetic     =', 3) ;  12.48868598
match ; External energy    ; GREPFIELD(static/info, 'External    =', 3) ;  -5.60532308

match ; k-point 1 (x) ; GREPFIELD(static/info, '#k =   1', 7) ; 0.25
match ; k-point 1 (y) ; GREPFIELD(static/info, '#k =   1', 8) ; 0.25
match ; k-point 1 (z) ; GREPFIELD(static/info, '#k =   1', 9) ; 0.25
match ; Eigenvalue  1 ; GREPFIELD(static/info, '#k =   1', 3, 1) ;  -0.275751
match ; Eigenvalue  8 ; GREPFIELD(static/info, '#k =   1', 3, 8) ;  -0.010816
match ; Eigenvalue 16 ; GREPFIELD(static/info, '#k =   1', 3, 16) ;  0.152233
match ; Eigenvalue 17 ; GREPFIELD(static/info, '#k =   1', 3, 17) ;  0.205432

match ; k-point 4 (x) ; GREPFIELD(static/info, '#k =   4', 7) ; 0.25
match ; k-point 4 (y) ; GREPFIELD(static/info, '#k =   4', 8) ; 0.25
match ; k-point 4 (z) ; GREPFIELD(static/info, '#k =   4', 9) ; -0.25
match ; Eigenvalue  1 ; GREPFIELD(static/info, '#k =   4', 3, 1) ;  -0.277124
match ; Eigenvalue  8 ; GREPFIELD(static/info, '#k =   4', 3, 8) ;   0.000422
match ; Eigenvalue 16 ; GREPFIELD(static/info, '#k =   4', 3, 16) ;  0.149264
match ; Eigenvalue 17 ; GREPFIELD(static/info, '#k =   4', 3, 17) ;  0.188701

match ; k-point 3 (x) ; GREPFIELD(static/info, '#k =   3', 7) ; 0.25
match ; k-point 3 (y) ; GREPFIELD(static/info, '#k =   3', 8) ; -0.25
match ; k-point 3 (z) ; GREPFIELD(static/info, '#k =   3', 9) ; 0.25
match ; Eigenvalue  1 ; GREPFIELD(static/info, '#k =   3', 3, 1) ;  -0.277421
match ; Eigenvalue  8 ; GREPFIELD(static/info, '#k =   3', 3, 8) ;   0.004466
match ; Eigenvalue 16 ; GREPFIELD(static/info, '#k =   3', 3, 16) ;  0.131700
match ; Eigenvalue 17 ; GREPFIELD(static/info, '#k =   3', 3, 17) ;  0.196784

match ; k-point 2 (x) ; GREPFIELD(static/info, '#k =   2', 7) ; -0.25
match ; k-point 2 (y) ; GREPFIELD(static/info, '#k =   2', 8) ; 0.25
match ; k-point 2 (z) ; GREPFIELD(static/info, '#k =   2', 9) ; 0.25
match ; Eigenvalue  1 ; GREPFIELD(static/info, '#k =   2', 3, 1) ;  -0.275750
match ; Eigenvalue  8 ; GREPFIELD(static/info, '#k =   2', 3, 8) ;  -0.009037
match ; Eigenvalue 16 ; GREPFIELD(static/info, '#k =   2', 3, 16) ;  0.152907
match ; Eigenvalue 17 ; GREPFIELD(static/info, '#k =   2', 3, 17) ;  0.202479

match ; Force 1 (x)   ; GREPFIELD(static/info, '1        Si', 3) ; -0.119568
match ; Force 1 (y)   ; GREPFIELD(static/info, '1        Si', 4) ; -0.017228
match ; Force 1 (z)   ; GREPFIELD(static/info, '1        Si', 5) ; -0.022951

match ; Force 2 (x)   ; GREPFIELD(static/info, '2        Si', 3) ;  0.009961
match ; Force 2 (y)   ; GREPFIELD(static/info, '2        Si', 4) ; -0.002430
match ; Force 2 (z)   ; GREPFIELD(static/info, '2        Si', 5) ; -0.005630

match ; Force 3 (x)   ; GREPFIELD(static/info, '3        Si', 3) ;  0.010170
match ; Force 3 (y)   ; GREPFIELD(static/info, '3        Si', 4) ;  0.005203
match ; Force 3 (z)   ; GREPFIELD(static/info, '3        Si', 5) ;  0.003385

match ; Force 4 (x)   ; GREPFIELD(static/info, '4        Si', 3) ; -0.016228
match ; Force 4 (y)   ; GREPFIELD(static/info, '4        Si', 4) ; -0.002371
match ; Force 4 (z)   ; GREPFIELD(static/info, '4        Si', 5) ;  0.002513

match ; Force 5 (x)   ; GREPFIELD(static/info, '5        Si', 3) ;  0.040874
match ; Force 5 (y)   ; GREPFIELD(static/info, '5        Si', 4) ;  0.053543
match ; Force 5 (z)   ; GREPFIELD(static/info, '5        Si', 5) ;  0.046550

match ; Force 6 (x)   ; GREPFIELD(static/info, '6        Si', 3) ;  0.023576
match ; Force 6 (y)   ; GREPFIELD(static/info, '6        Si', 4) ;  0.011646
match ; Force 6 (z)   ; GREPFIELD(static/info, '6        Si', 5) ; -0.010608

match ; Force 7 (x)   ; GREPFIELD(static/info, '7        Si', 3) ;  0.025137
match ; Force 7 (y)   ; GREPFIELD(static/info, '7        Si', 4) ; -0.019312
match ; Force 7 (z)   ; GREPFIELD(static/info, '7        Si', 5) ;  0.019852

match ; Force 8 (x)   ; GREPFIELD(static/info, '8        Si', 3) ;  0.029250
match ; Force 8 (y)   ; GREPFIELD(static/info, '8        Si', 4) ; -0.029204
match ; Force 8 (z)   ; GREPFIELD(static/info, '8        Si', 5) ; -0.032381

Precision : 1.0e-06
match ; Stress (xx)   ; GREPFIELD(static/info, 'T_{ij}', 2, 1) ; -1.267587E-04
match ; Stress (yy)   ; GREPFIELD(static/info, 'T_{ij}', 3, 2) ; -2.643272E-04
match ; Stress (zz)   ; GREPFIELD(static/info, 'T_{ij}', 4, 3) ; -2.549701E-04
match ; Stress (xy)   ; GREPFIELD(static/info, 'T_{ij}', 3, 1) ; -3.582628E-05
match ; Stress (yx)   ; GREPFIELD(static/info, 'T_{ij}', 2, 2) ; -3.611897E-05
match ; Stress (yz)   ; GREPFIELD(static/info, 'T_{ij}', 4, 2) ; -1.604207E-04
match ; Stress (zy)   ; GREPFIELD(static/info, 'T_{ij}', 3, 3) ; -1.605365E-04
match ; Stress (zx)   ; GREPFIELD(static/info, 'T_{ij}', 2, 3) ;  1.236401E-06 
match ; Stress (xz)   ; GREPFIELD(static/info, 'T_{ij}', 4, 1) ;  1.218545E-06 
