# -*- coding: utf-8 mode: shell-script -*-

Test       : Silicon force
Program    : octopus
TestGroups : short-run, periodic_systems
Enabled    : Yes

Input      : 11-silicon_force.01-gs.inp

match ; Total k-points   ; GREPFIELD(static/info, 'Total number of k-points', 6) ; 8
match ; Reduced k-points ; GREPFIELD(static/info, 'Number of symmetry-reduced k-points', 6) ; 4
match ; Space group        ; GREPFIELD(out, 'Space group', 4) ; 1
match ; No. of symmetries  ; GREPFIELD(out, 'symmetries that can be used', 5)  ;  1

match ; Total energy       ; GREPFIELD(static/info, 'Total       =', 3) ; -32.05472546
match ; Ion-ion energy     ; GREPFIELD(static/info, 'Ion-ion     =', 3) ; -31.21044509
match ; Eigenvalues sum    ; GREPFIELD(static/info, 'Eigenvalues =', 3) ;  -1.33251731
match ; Hartree energy     ; GREPFIELD(static/info, 'Hartree     =', 3) ;   2.45522240
match ; Exchange energy    ; GREPFIELD(static/info, 'Exchange    =', 3) ;  -8.17896907
match ; Correlation energy ; GREPFIELD(static/info, 'Correlation =', 3) ;  -1.50276656
match ; Kinetic energy     ; GREPFIELD(static/info, 'Kinetic     =', 3) ;  12.42046688
match ; External energy    ; GREPFIELD(static/info, 'External    =', 3) ;  -5.58660616

match ; k-point 1 (x) ; GREPFIELD(static/info, '#k =   1', 7) ; 0.25
match ; k-point 1 (y) ; GREPFIELD(static/info, '#k =   1', 8) ; 0.25
match ; k-point 1 (z) ; GREPFIELD(static/info, '#k =   1', 9) ; 0.25
match ; Eigenvalue  1 ; GREPFIELD(static/info, '#k =   1', 3, 1) ;  -0.276362
match ; Eigenvalue  8 ; GREPFIELD(static/info, '#k =   1', 3, 8) ;  -0.014123
match ; Eigenvalue 16 ; GREPFIELD(static/info, '#k =   1', 3, 16) ;  0.149377
match ; Eigenvalue 17 ; GREPFIELD(static/info, '#k =   1', 3, 17) ;  0.198000

match ; k-point 4 (x) ; GREPFIELD(static/info, '#k =   4', 7) ; 0.25
match ; k-point 4 (y) ; GREPFIELD(static/info, '#k =   4', 8) ; 0.25
match ; k-point 4 (z) ; GREPFIELD(static/info, '#k =   4', 9) ; -0.25
match ; Eigenvalue  1 ; GREPFIELD(static/info, '#k =   4', 3, 1) ;  -0.277745
match ; Eigenvalue  8 ; GREPFIELD(static/info, '#k =   4', 3, 8) ;  -0.003448
match ; Eigenvalue 16 ; GREPFIELD(static/info, '#k =   4', 3, 16) ;  0.146686
match ; Eigenvalue 17 ; GREPFIELD(static/info, '#k =   4', 3, 17) ;  0.182427

match ; k-point 3 (x) ; GREPFIELD(static/info, '#k =   3', 7) ; 0.25
match ; k-point 3 (y) ; GREPFIELD(static/info, '#k =   3', 8) ; -0.25
match ; k-point 3 (z) ; GREPFIELD(static/info, '#k =   3', 9) ; 0.25
match ; Eigenvalue  1 ; GREPFIELD(static/info, '#k =   3', 3, 1) ;  -0.278007
match ; Eigenvalue  8 ; GREPFIELD(static/info, '#k =   3', 3, 8) ;   0.000493
match ; Eigenvalue 16 ; GREPFIELD(static/info, '#k =   3', 3, 16) ;  0.128628
match ; Eigenvalue 17 ; GREPFIELD(static/info, '#k =   3', 3, 17) ;  0.189691

match ; k-point 2 (x) ; GREPFIELD(static/info, '#k =   2', 7) ; -0.25
match ; k-point 2 (y) ; GREPFIELD(static/info, '#k =   2', 8) ; 0.25
match ; k-point 2 (z) ; GREPFIELD(static/info, '#k =   2', 9) ; 0.25
match ; Eigenvalue  1 ; GREPFIELD(static/info, '#k =   2', 3, 1) ;  -0.276371
match ; Eigenvalue  8 ; GREPFIELD(static/info, '#k =   2', 3, 8) ;  -0.012351
match ; Eigenvalue 16 ; GREPFIELD(static/info, '#k =   2', 3, 16) ;  0.150110
match ; Eigenvalue 17 ; GREPFIELD(static/info, '#k =   2', 3, 17) ;  0.195664

match ; Force 1 (x)   ; GREPFIELD(static/info, '1        Si', 3) ; -0.042730
match ; Force 1 (y)   ; GREPFIELD(static/info, '1        Si', 4) ; -0.021465
match ; Force 1 (z)   ; GREPFIELD(static/info, '1        Si', 5) ; -0.005103

match ; Force 2 (x)   ; GREPFIELD(static/info, '2        Si', 3) ; -0.020950
match ; Force 2 (y)   ; GREPFIELD(static/info, '2        Si', 4) ;  0.010733
match ; Force 2 (z)   ; GREPFIELD(static/info, '2        Si', 5) ;  0.002700

match ; Force 3 (x)   ; GREPFIELD(static/info, '3        Si', 3) ; -0.021628
match ; Force 3 (y)   ; GREPFIELD(static/info, '3        Si', 4) ; -0.007627
match ; Force 3 (z)   ; GREPFIELD(static/info, '3        Si', 5) ; -0.010347

match ; Force 4 (x)   ; GREPFIELD(static/info, '4        Si', 3) ; -0.003621
match ; Force 4 (y)   ; GREPFIELD(static/info, '4        Si', 4) ;  0.010805
match ; Force 4 (z)   ; GREPFIELD(static/info, '4        Si', 5) ; -0.008762

match ; Force 5 (x)   ; GREPFIELD(static/info, '5        Si', 3) ;  0.039790
match ; Force 5 (y)   ; GREPFIELD(static/info, '5        Si', 4) ;  0.045389
match ; Force 5 (z)   ; GREPFIELD(static/info, '5        Si', 5) ;  0.045388

match ; Force 6 (x)   ; GREPFIELD(static/info, '6        Si', 3) ;  0.008460
match ; Force 6 (y)   ; GREPFIELD(static/info, '6        Si', 4) ; -0.023722
match ; Force 6 (z)   ; GREPFIELD(static/info, '6        Si', 5) ;  0.025362

match ; Force 7 (x)   ; GREPFIELD(static/info, '7        Si', 3) ;  0.002213
match ; Force 7 (y)   ; GREPFIELD(static/info, '7        Si', 4) ;  0.023748
match ; Force 7 (z)   ; GREPFIELD(static/info, '7        Si', 5) ; -0.020579

match ; Force 8 (x)   ; GREPFIELD(static/info, '8        Si', 3) ;  0.033565
match ; Force 8 (y)   ; GREPFIELD(static/info, '8        Si', 4) ; -0.035009
match ; Force 8 (z)   ; GREPFIELD(static/info, '8        Si', 5) ; -0.031657

Precision : 1.0e-06
match ; Stress (xx)   ; GREPFIELD(static/info, 'T_{ij}', 2, 1) ;  -7.602000E-05
match ; Stress (yy)   ; GREPFIELD(static/info, 'T_{ij}', 3, 2) ;  -1.936876E-04
match ; Stress (zz)   ; GREPFIELD(static/info, 'T_{ij}', 4, 3) ;  -1.861335E-04
match ; Stress (xy)   ; GREPFIELD(static/info, 'T_{ij}', 3, 1) ;  -3.198474E-05
match ; Stress (yx)   ; GREPFIELD(static/info, 'T_{ij}', 2, 2) ;  -3.235769E-05
match ; Stress (yz)   ; GREPFIELD(static/info, 'T_{ij}', 4, 2) ;  -1.106587E-04
match ; Stress (zy)   ; GREPFIELD(static/info, 'T_{ij}', 3, 3) ;  -1.105807E-04
match ; Stress (zx)   ; GREPFIELD(static/info, 'T_{ij}', 2, 3) ;   4.199065E-06
match ; Stress (xz)   ; GREPFIELD(static/info, 'T_{ij}', 4, 1) ;   4.215260E-06
