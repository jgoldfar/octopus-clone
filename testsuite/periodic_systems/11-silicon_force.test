# -*- coding: utf-8 mode: shell-script -*-
# $Id$

Test       : Silicon force
Program    : octopus
TestGroups : short-run, periodic_systems
Enabled    : Yes

Input      : 11-silicon_force.01-gs.inp

match ; Total k-points   ; GREPFIELD(static/info, 'Total number of k-points', 6) ; 8
match ; Reduced k-points ; GREPFIELD(static/info, 'Number of symmetry-reduced k-points', 6) ; 8
match ; Space group        ; GREPFIELD(out, 'Space group', 4) ; 1
match ; No. of symmetries  ; GREPFIELD(out, 'symmetries that can be used', 5)  ;  1

match ; Total energy       ; GREPFIELD(static/info, 'Total       =', 3) ; -34.21402984
match ; Ion-ion energy     ; GREPFIELD(static/info, 'Ion-ion     =', 3) ; -33.57875943
match ; Eigenvalues sum    ; GREPFIELD(static/info, 'Eigenvalues =', 3) ; -1.41198384
match ; Hartree energy     ; GREPFIELD(static/info, 'Hartree     =', 3) ; 2.12949053
match ; Exchange energy    ; GREPFIELD(static/info, 'Exchange    =', 3) ; -8.06785218
match ; Correlation energy ; GREPFIELD(static/info, 'Correlation =', 3) ; -1.49544943
match ; Kinetic energy     ; GREPFIELD(static/info, 'Kinetic     =', 3) ; 12.69060953
match ; External energy    ; GREPFIELD(static/info, 'External    =', 3) ; -5.96931873

