# -*- coding: utf-8 mode: shell-script -*-

Test       : Boron Nitride Sheet
Program    : pulpito
TestGroups : short-run, periodic_systems
Enabled    : Yes

Processors : 4

Input      : 12-boron_nitride.01-gs.inp

match ; SCF convergence ; GREPCOUNT(static/info, 'SCF converged') ; 1

match ; Total k-points     ; GREPFIELD(static/info, 'Total number of k-points', 6) ; 4
match ; Reduced k-points   ; GREPFIELD(static/info, 'Number of symmetry-reduced k-points', 6) ; 4
match ; Space group        ; GREPFIELD(out, 'Space group', 4) ; 187
match ; No. of symmetries  ; GREPFIELD(out, 'symmetries that can be used', 5)  ;   4

match ; Total energy       ; GREPFIELD(static/info, 'Total       =', 3) ; -708.39628070
match ; Free energy        ; GREPFIELD(static/info, 'Free        =', 3) ; -708.39628070
match ; Ion-ion energy     ; GREPFIELD(static/info, 'Ion-ion     =', 3) ; -1086.63818887
match ; Eigenvalues sum    ; GREPFIELD(static/info, 'Eigenvalues =', 3) ; -201.03924143
match ; Hartree energy     ; GREPFIELD(static/info, 'Hartree     =', 3) ; -523.82482841
match ; Exchange energy    ; GREPFIELD(static/info, 'Exchange    =', 3) ; -181.50659613
match ; Correlation energy ; GREPFIELD(static/info, 'Correlation =', 3) ; -25.09122070
match ; Kinetic energy     ; GREPFIELD(static/info, 'Kinetic     =', 3) ; 516.13543547
match ; External energy    ; GREPFIELD(static/info, 'External    =', 3) ; 592.52922871
match ; Entropy            ; GREPFIELD(static/info, 'Entropy     =', 3) ; 0.00000000
match ; Fermi energy       ; GREPFIELD(static/info, 'Fermi energy =', 4); -4.467322

match ; k-point 1 (x) ; GREPFIELD(static/info, '#k =   1', 7) ;   0.000000
match ; k-point 1 (y) ; GREPFIELD(static/info, '#k =   1', 8) ;   0.000000
match ; k-point 1 (z) ; GREPFIELD(static/info, '#k =   1', 9) ;   0.000000
match ; Eigenvalue  1 ; GREPFIELD(static/info, '#k =   1', 3, 1) ; -22.844435
match ; Eigenvalue  8 ; GREPFIELD(static/info, '#k =   1', 3, 8) ; -5.565652
match ; Eigenvalue  9 ; GREPFIELD(static/info, '#k =   1', 3, 9) ; -1.102717
match ; Eigenvalue 10 ; GREPFIELD(static/info, '#k =   1', 3, 10) ; 0.961305

match ; k-point 4 (x) ; GREPFIELD(static/info, '#k =   4', 7) ;   0.500000
match ; k-point 4 (y) ; GREPFIELD(static/info, '#k =   4', 8) ;   0.500000
match ; k-point 4 (z) ; GREPFIELD(static/info, '#k =   4', 9) ;   0.000000
match ; Eigenvalue  1 ; GREPFIELD(static/info, '#k =   4', 3, 1) ; -19.993008
match ; Eigenvalue  8 ; GREPFIELD(static/info, '#k =   4', 3, 8) ;  -7.452304
match ; Eigenvalue  9 ; GREPFIELD(static/info, '#k =   4', 3, 9) ; -1.091021
match ; Eigenvalue 10 ; GREPFIELD(static/info, '#k =   4', 3, 10) ; -1.091021

# forces on 1 and 3; 2 and 4 must be equal by symmetry
match ; Force 1 (x)   ; GREPFIELD(static/info, '1         B', 3)  ;  0.861621
match ; Force 1 (y)   ; GREPFIELD(static/info, '1         B', 4)  ;   0
match ; Force 1 (z)   ; GREPFIELD(static/info, '1         B', 5)  ;   0

match ; Force 2 (x)   ; GREPFIELD(static/info, '2         N', 3)  ;  0.353480
match ; Force 2 (y)   ; GREPFIELD(static/info, '2         N', 4)  ;   0
match ; Force 2 (z)   ; GREPFIELD(static/info, '2         N', 5)  ;   0

match ; Force 3 (x)   ; GREPFIELD(static/info, '3         B', 3)  ;  0.861621
match ; Force 3 (y)   ; GREPFIELD(static/info, '3         B', 4)  ;   0
match ; Force 3 (z)   ; GREPFIELD(static/info, '3         B', 5)  ;   0

match ; Force 4 (x)   ; GREPFIELD(static/info, '4         N', 3)  ;  0.353477
match ; Force 4 (y)   ; GREPFIELD(static/info, '4         N', 4)  ;   0
match ; Force 4 (z)   ; GREPFIELD(static/info, '4         N', 5)  ;   0
