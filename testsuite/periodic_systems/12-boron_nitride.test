# -*- coding: utf-8 mode: shell-script -*-
# $Id$

Test       : Boron Nitride Sheet
Program    : octopus
TestGroups : short-run, periodic_systems
Enabled    : Yes

Processors : 4

Input      : 12-boron_nitride.01-gs.inp

match ; Total k-points     ; GREPFIELD(static/info, 'Total number of k-points', 6) ; 4; 0.0001
match ; Reduced k-points   ; GREPFIELD(static/info, 'Number of symmetry-reduced k-points', 6) ; 4; 0.0001
match ; Space group        ; GREPFIELD(out, 'Space group', 4) ; 187; 0.0001
match ; No. of symmetries  ; GREPFIELD(out, 'symmetries that can be used', 5)  ;   4; 0.0001

match ; Total energy       ; GREPFIELD(static/info, 'Total       =', 3) ; -699.52841943; 0.0001
match ; Free energy        ; GREPFIELD(static/info, 'Free        =', 3) ; -699.52841943; 0.0001
match ; Ion-ion energy     ; GREPFIELD(static/info, 'Ion-ion     =', 3) ; -1086.63818887; 0.0001
match ; Eigenvalues sum    ; GREPFIELD(static/info, 'Eigenvalues =', 3) ; -198.67050830; 0.0001
match ; Hartree energy     ; GREPFIELD(static/info, 'Hartree     =', 3) ; -524.27936356; 0.0001
match ; Exchange energy    ; GREPFIELD(static/info, 'Exchange    =', 3) ; -174.81774097; 0.0001
match ; Correlation energy ; GREPFIELD(static/info, 'Correlation =', 3) ;  -24.25113377; 0.0001
match ; Kinetic energy     ; GREPFIELD(static/info, 'Kinetic     =', 3) ;  516.48055412; 0.0001
match ; External energy    ; GREPFIELD(static/info, 'External    =', 3) ;  593.97768047; 0.0001
match ; Entropy            ; GREPFIELD(static/info, 'Entropy     =', 3) ;    0.0; 0.0001
match ; Fermi energy       ; GREPFIELD(static/info, 'Fermi energy =', 4);   -1.383534; 0.0001

match ; k-point 1 (x) ; GREPFIELD(static/info, '#k =   1', 7) ;   0.000000; 0.0001
match ; k-point 1 (y) ; GREPFIELD(static/info, '#k =   1', 8) ;   0.000000; 0.0001
match ; k-point 1 (z) ; GREPFIELD(static/info, '#k =   1', 9) ;   0.000000; 0.0001
match ; Eigenvalue  1 ; GREPFIELD(static/info, '#k =   1', 3, 1) ; -23.031550; 0.0001
match ; Eigenvalue  8 ; GREPFIELD(static/info, '#k =   1', 3, 8) ; -4.555298; 0.0001
match ; Eigenvalue  9 ; GREPFIELD(static/info, '#k =   1', 3, 9) ; -1.053529; 0.0001
match ; Eigenvalue 10 ; GREPFIELD(static/info, '#k =   1', 3, 10) ; 0.970591; 0.0001

match ; k-point 4 (x) ; GREPFIELD(static/info, '#k =   4', 7) ;   0.500000; 0.0001
match ; k-point 4 (y) ; GREPFIELD(static/info, '#k =   4', 8) ;   0.500000; 0.0001
match ; k-point 4 (z) ; GREPFIELD(static/info, '#k =   4', 9) ;   0.000000; 0.0001
match ; Eigenvalue  1 ; GREPFIELD(static/info, '#k =   4', 3, 1) ; -19.693686; 0.0001
match ; Eigenvalue  8 ; GREPFIELD(static/info, '#k =   4', 3, 8) ; -7.384803; 0.0001
match ; Eigenvalue  9 ; GREPFIELD(static/info, '#k =   4', 3, 9) ; -1.035103; 0.0001
match ; Eigenvalue 10 ; GREPFIELD(static/info, '#k =   4', 3, 10) ; -1.035103; 0.0001

# forces on 1 and 3; 2 and 4 must be equal by symmetry
match ; Force 1 (x)   ; GREPFIELD(static/info, '1         B', 3)  ;  -0.165918; 0.0001
match ; Force 1 (y)   ; GREPFIELD(static/info, '1         B', 4)  ;   0; 0.0001
match ; Force 1 (z)   ; GREPFIELD(static/info, '1         B', 5)  ;   0; 0.0001

match ; Force 2 (x)   ; GREPFIELD(static/info, '2         N', 3)  ;   1.419550; 0.0001
match ; Force 2 (y)   ; GREPFIELD(static/info, '2         N', 4)  ;   0; 0.0001
match ; Force 2 (z)   ; GREPFIELD(static/info, '2         N', 5)  ;   0; 0.0001

match ; Force 3 (x)   ; GREPFIELD(static/info, '3         B', 3)  ;  -0.165916; 0.0001
match ; Force 3 (y)   ; GREPFIELD(static/info, '3         B', 4)  ;   0; 0.0001
match ; Force 3 (z)   ; GREPFIELD(static/info, '3         B', 5)  ;   0; 0.0001

match ; Force 4 (x)   ; GREPFIELD(static/info, '4         N', 3)  ;   1.419558; 0.0001
match ; Force 4 (y)   ; GREPFIELD(static/info, '4         N', 4)  ;   0; 0.0001
match ; Force 4 (z)   ; GREPFIELD(static/info, '4         N', 5)  ;   0; 0.0001
