# -*- coding: utf-8 mode: shell-script -*-
# $Id$

Test       : BerkeleyGW
Options    : berkeleygw
Program    : octopus
TestGroups : long-run, periodic_systems
Enabled    : Yes

Processors : 4

Input      : 10-berkeleygw.01-gs.inp

match ; Total k-points   ; GREPFIELD(static/info, 'Total number of k-points', 6) ; 8; 0.0001

match ; Total energy       ; GREPFIELD(static/info, 'Total       =', 3) ; -31.7794479; 0.0001
match ; Ion-ion energy     ; GREPFIELD(static/info, 'Ion-ion     =', 3) ; -31.4312028; 0.0001
match ; Eigenvalues sum    ; GREPFIELD(static/info, 'Eigenvalues =', 3) ; -1.06745253; 0.0001
match ; Hartree energy     ; GREPFIELD(static/info, 'Hartree     =', 3) ; 2.21105822; 0.0001
match ; Exchange energy    ; GREPFIELD(static/info, 'Exchange    =', 3) ; -8.13952857; 0.0001
match ; Correlation energy ; GREPFIELD(static/info, 'Correlation =', 3) ; -1.50024588; 0.0001
match ; Kinetic energy     ; GREPFIELD(static/info, 'Kinetic     =', 3) ; 12.47687684; 0.0001
match ; External energy    ; GREPFIELD(static/info, 'External    =', 3) ; -5.39639832; 0.0001

match ; k-point 1 (x) ; GREPFIELD(static/info, '#k =   1', 7) ;  0.250000; 0.0001
match ; k-point 1 (y) ; GREPFIELD(static/info, '#k =   1', 8) ;  0.250000; 0.0001
match ; k-point 1 (z) ; GREPFIELD(static/info, '#k =   1', 9) ;  0.250000; 0.0001
match ; Eigenvalue  1 ; GREPFIELD(static/info, '#k =   1', 3, 1) ; -0.257309; 0.0001
match ; Eigenvalue  8 ; GREPFIELD(static/info, '#k =   1', 3, 8) ; 0.008457; 0.0001
match ; Eigenvalue 16 ; GREPFIELD(static/info, '#k =   1', 3, 16) ; 0.123205; 0.0001
match ; Eigenvalue 18 ; GREPFIELD(static/info, '#k =   1', 3, 18) ; 0.196285; 0.0001

match ; k-point 2 (x) ; GREPFIELD(static/info, '#k =   2', 7) ;  0.250000; 0.0001
match ; k-point 2 (y) ; GREPFIELD(static/info, '#k =   2', 8) ;  0.250000; 0.0001
match ; k-point 2 (z) ; GREPFIELD(static/info, '#k =   2', 9) ; -0.250000; 0.0001
match ; Eigenvalue  1 ; GREPFIELD(static/info, '#k =   2', 3, 1) ; -0.25730; 0.0001
match ; Eigenvalue  8 ; GREPFIELD(static/info, '#k =   2', 3, 8) ; 0.008457; 0.0001
match ; Eigenvalue 16 ; GREPFIELD(static/info, '#k =   2', 3, 16) ; 0.123205; 0.0001
match ; Eigenvalue 18 ; GREPFIELD(static/info, '#k =   2', 3, 18) ; 0.196285; 0.0001

# These files are binary, so we cannot easily test the contents.
match ; WFN  ; SIZE(static/WFN)  ;  337964; 0.0001
match ; RHO  ; SIZE(static/RHO)  ;   41812; 0.0001
match ; VXC  ; SIZE(static/VXC)  ;   41812; 0.0001

# The two k-points are equivalent by time-reversal symmetry,
# so results should be identical between them.

match ; vxc k 1 z       ; LINEFIELD(static/vxc.dat,   1, 3);     0.25; 0.0001
match ; vxc k 1 diag    ; LINEFIELD(static/vxc.dat,   1, 4);    18; 0.0001
match ; vxc k 1 offdiag ; LINEFIELD(static/vxc.dat,   1, 5);   324; 0.0001
match ; vxc k 1 2 Re    ; LINEFIELD(static/vxc.dat,   3, 3);   -10.77421748; 0.0001
match ; vxc k 1 2 Im    ; LINEFIELD(static/vxc.dat,   3, 4);     0.0; 0.0001
match ; vxc k 1 8 8 Re  ; LINEFIELD(static/vxc.dat, 153, 4);   -10.6717566; 0.0001
match ; vxc k 1 8 8 Im  ; LINEFIELD(static/vxc.dat, 153, 5);     0.0; 0.0001
# unfortunately, offdiagonals are dependent on arbitrary phases, cannot be tested

match ; vxc k 2 z       ; LINEFIELD(static/vxc.dat, 344, 3);    -0.25; 0.0001
match ; vxc k 2 diag    ; LINEFIELD(static/vxc.dat, 344, 4);    18; 0.0001
match ; vxc k 2 offdiag ; LINEFIELD(static/vxc.dat, 344, 5);   324; 0.0001
match ; vxc k 1 1 Re    ; LINEFIELD(static/vxc.dat, 352, 3);   -10.671756; 0.0001
match ; vxc k 1 1 Im    ; LINEFIELD(static/vxc.dat, 352, 4);     0.0; 0.0001



Input      : 10-berkeleygw.02-unocc.inp

match ; k-point 1 (x) ; GREPFIELD(static/eigenvalues, '#k =   1', 7) ;  0.0000; 0.0001
