# -*- coding: utf-8 mode: shell-script -*-

Test       : Hybrid functional for periodic systems
Options    : libisf
Program    : octopus
TestGroups : long-run, periodic_systems
Enabled    : Yes

#We only have two k-points
Processors : 2

Input      : 17-hybrids_psolver.01-Si_gs.inp

match ; Total k-points   ; GREPFIELD(static/info, 'Total number of k-points', 6) ; 8
match ; Reduced k-points ; GREPFIELD(static/info, 'Number of symmetry-reduced k-points', 6) ; 8
match ; Space group        ; GREPFIELD(out, 'Space group', 4) ; 227
match ; No. of symmetries  ; GREPFIELD(out, 'symmetries that can be used', 5)  ;  24

match ; Total energy       ; GREPFIELD(static/info, 'Total       =', 3) ; -7.51450570
match ; Ion-ion energy     ; GREPFIELD(static/info, 'Ion-ion     =', 3) ; -7.81793472
match ; Eigenvalues sum    ; GREPFIELD(static/info, 'Eigenvalues =', 3) ;  0.27204381
match ; Hartree energy     ; GREPFIELD(static/info, 'Hartree     =', 3) ;  0.60082528
match ; Exchange energy    ; GREPFIELD(static/info, 'Exchange    =', 3) ;  0.00000000
match ; Correlation energy ; GREPFIELD(static/info, 'Correlation =', 3) ; -2.01372259
match ; Kinetic energy     ; GREPFIELD(static/info, 'Kinetic     =', 3) ;  3.21808779
match ; External energy    ; GREPFIELD(static/info, 'External    =', 3) ; -1.41971558

match ; k-point 1 (x) ; GREPFIELD(static/info, '#k =   1', 7) ; 0.000000
match ; k-point 1 (y) ; GREPFIELD(static/info, '#k =   1', 8) ; 0.000000
match ; k-point 1 (z) ; GREPFIELD(static/info, '#k =   1', 9) ; 0.000000
match ; Eigenvalue  1 ; GREPFIELD(static/info, '#k =   1', 3, 1) ; -0.248620
match ; Eigenvalue  2 ; GREPFIELD(static/info, '#k =   1', 3, 2) ;  0.197295
match ; Eigenvalue  4 ; GREPFIELD(static/info, '#k =   1', 3, 4) ;  0.197477
match ; Eigenvalue  5 ; GREPFIELD(static/info, '#k =   1', 3, 5) ;  0.307831

match ; k-point 2 (x) ; GREPFIELD(static/info, '#k =   2', 7) ; 0.000000
match ; k-point 2 (y) ; GREPFIELD(static/info, '#k =   2', 8) ; 0.500000
match ; k-point 2 (z) ; GREPFIELD(static/info, '#k =   2', 9) ; 0.000000
match ; Eigenvalue  1 ; GREPFIELD(static/info, '#k =   2', 3, 1) ; -0.152951
match ; Eigenvalue  2 ; GREPFIELD(static/info, '#k =   2', 3, 2) ; -0.050780
match ; Eigenvalue  4 ; GREPFIELD(static/info, '#k =   2', 3, 4) ;  0.164740
match ; Eigenvalue  5 ; GREPFIELD(static/info, '#k =   2', 3, 5) ;  0.273400

match ; k-point 5 (x) ; GREPFIELD(static/info, '#k =   5', 7) ; 0.000000
match ; k-point 5 (y) ; GREPFIELD(static/info, '#k =   5', 8) ; 0.500000
match ; k-point 5 (z) ; GREPFIELD(static/info, '#k =   5', 9) ; 0.500000
match ; Eigenvalue  1 ; GREPFIELD(static/info, '#k =   5', 3, 1) ; -0.084074
match ; Eigenvalue  2 ; GREPFIELD(static/info, '#k =   5', 3, 2) ; -0.066470
match ; Eigenvalue  4 ; GREPFIELD(static/info, '#k =   5', 3, 4) ;  0.108969
match ; Eigenvalue  5 ; GREPFIELD(static/info, '#k =   5', 3, 5) ;  0.227114

match ; DOS E Fermi    ; LINEFIELD(static/total-dos-efermi.dat, 2, 1);   0.197477
match ; DOS energy 2   ; LINEFIELD(static/total-dos.dat,   2, 1);       -0.387733
match ; DOS value  2   ; LINEFIELD(static/total-dos.dat,   2, 2);        0.094375
match ; DOS energy 442 ; LINEFIELD(static/total-dos.dat, 442, 1);        0.348255
match ; DOS value  442 ; LINEFIELD(static/total-dos.dat, 442, 2);        0.641979
