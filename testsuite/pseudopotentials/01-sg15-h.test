# -*- coding: utf-8 mode: shell-script -*-
# $Id$

Test       : SG15: hydrogen
Program    : octopus
TestGroups : short-run, pseudopotentials
Enabled    : Yes

Input      : 01-sg15-h.01-gs.inp
match ; Total energy       ; GREP(static/info, 'Total       =', 20) ; -0.44602615
match ; Eigenvalues sum    ; GREP(static/info, 'Eigenvalues =', 20) ; -0.23311090
match ; Hartree energy     ; GREP(static/info, 'Hartree     =', 20) ;  0.28362035
match ; Exchange energy    ; GREP(static/info, 'Exchange    =', 20) ; -0.19351844
match ; Correlation energy ; GREP(static/info, 'Correlation =', 20) ; -0.03970618
match ; Kinetic energy     ; GREP(static/info, 'Kinetic     =', 20) ;  0.42125337
match ; External energy    ; GREP(static/info, 'External    =', 20) ; -0.91767516
match ; Non-local energy   ; GREP(static/info, 'Non-local   =', 20) ; -0.03260742
match ; Eigenvalue         ; GREP(static/info, ' 1   --', 13)       ; -0.233111
