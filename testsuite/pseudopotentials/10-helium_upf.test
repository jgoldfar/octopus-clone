# -*- coding: utf-8 mode: shell-script -*-

Test       : Helium UPF
Program    : octopus
TestGroups : short-run, pseudopotentials
Enabled    : Yes

ExtraFile: 10-helium_upf.pbe-mt.UPF
Input: 10-helium_upf.01-gs.inp
Precision: 0.00011
match ; Total energy       ; GREPFIELD(static/info, 'Total       =', 3) ; -2.83763329
match ; Ion-ion energy     ; GREPFIELD(static/info, 'Ion-ion     =', 3) ;  0.00000000
match ; Eigenvalues sum    ; GREPFIELD(static/info, 'Eigenvalues =', 3) ; -1.14324249
match ; Hartree energy     ; GREPFIELD(static/info, 'Hartree     =', 3) ;  1.99478473
match ; Int[n*v_xc]        ; GREPFIELD(static/info, 'Int\[n\*v_xc\] =', 3) ; -1.26961787
match ; Exchange energy    ; GREPFIELD(static/info, 'Exchange    =', 3) ; -0.85818564
match ; Correlation energy ; GREPFIELD(static/info, 'Correlation =', 3) ; -0.11103830
match ; Kinetic energy     ; GREPFIELD(static/info, 'Kinetic     =', 3) ;  2.57373458
match ; External energy    ; GREPFIELD(static/info, 'External    =', 3) ; -6.43692872
match ; Eigenvalue 1       ; GREPFIELD(static/info, '  1   --', 3) ;      -0.571621

