# -*- coding: utf-8 mode: shell-script -*-

Test       : Real space symmetrization 
Program    : octopus
TestGroups : long-run, periodic_systems, symmetries
Enabled    : Yes

Input : 11-symmetrization.01-spg143_nosym.inp

match ; Total energy       ; GREPFIELD(static/info, 'Total       =', 3) ; -0.13029123
match ; Ion-ion energy     ; GREPFIELD(static/info, 'Ion-ion     =', 3) ; -0.13210182
match ; Eigenvalues sum    ; GREPFIELD(static/info, 'Eigenvalues =', 3) ; -0.11709504
match ; Hartree energy     ; GREPFIELD(static/info, 'Hartree     =', 3) ;  0.04001005
match ; Exchange energy    ; GREPFIELD(static/info, 'Exchange    =', 3) ; -0.44248849
match ; Correlation energy ; GREPFIELD(static/info, 'Correlation =', 3) ; -0.09064247
match ; Kinetic energy     ; GREPFIELD(static/info, 'Kinetic     =', 3) ;  0.38903636
match ; External energy    ; GREPFIELD(static/info, 'External    =', 3) ;  0.10589366

match ; Eigenvalue [  k=1, n=1   ] ; GREPFIELD(static/info, '#k =   1', 3, 1) ; -0.134574
match ; Eigenvalue [  k=1, n=2   ] ; GREPFIELD(static/info, '#k =   1', 3, 2) ;  0.073472
match ; Eigenvalue [  k=1, n=3   ] ; GREPFIELD(static/info, '#k =   1', 3, 3) ;  0.137029

match ; Eigenvalue [  k=2, n=1   ] ; GREPFIELD(static/info, '#k =   2', 3, 1) ; -0.089846
match ; Eigenvalue [  k=2, n=2   ] ; GREPFIELD(static/info, '#k =   2', 3, 2) ;  0.042613
match ; Eigenvalue [  k=2, n=3   ] ; GREPFIELD(static/info, '#k =   2', 3, 3) ;  0.115988

match ; Force 1 (x)   ; GREPFIELD(static/info, '1        Na', 3) ;  0.172289
match ; Force 1 (y)   ; GREPFIELD(static/info, '1        Na', 4) ;  0.237187
match ; Force 1 (z)   ; GREPFIELD(static/info, '1        Na', 5) ; -0.000026
match ; Force 2 (x)   ; GREPFIELD(static/info, '2        Na', 3) ;  0.118588
match ; Force 2 (y)   ; GREPFIELD(static/info, '2        Na', 4) ; -0.267545
match ; Force 2 (z)   ; GREPFIELD(static/info, '2        Na', 5) ; -0.000202
match ; Force 3 (x)   ; GREPFIELD(static/info, '3        Na', 3) ; -0.290965
match ; Force 3 (y)   ; GREPFIELD(static/info, '3        Na', 4) ;  0.030213
match ; Force 3 (z)   ; GREPFIELD(static/info, '3        Na', 5) ;  0.000226


