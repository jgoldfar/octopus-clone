match ; Eigenvalue [  k=1, n=4   ] ; GREPFIELD(static/info, '#k =   1', 3, 4) ;  0.274540
match ; Eigenvalue [  k=1, n=5   ] ; GREPFIELD(static/info, '#k =   1', 3, 5) ;  0.274547
match ; Eigenvalue [  k=1, n=6   ] ; GREPFIELD(static/info, '#k =   1', 3, 6) ;  0.274551

match ; Force 1 (x)   ; GREPFIELD(static/info, '1        Na', 3) ;  3.238461
match ; Force 1 (y)   ; GREPFIELD(static/info, '1        Na', 4) ;  1.046830
match ; Force 1 (z)   ; GREPFIELD(static/info, '1        Na', 5) ; -3.329696
match ; Force 2 (x)   ; GREPFIELD(static/info, '2        Na', 3) ; -3.330454
match ; Force 2 (y)   ; GREPFIELD(static/info, '2        Na', 4) ;  3.236884
match ; Force 2 (z)   ; GREPFIELD(static/info, '2        Na', 5) ;  1.047827
match ; Force 3 (x)   ; GREPFIELD(static/info, '3        Na', 3) ; -3.237105
match ; Force 3 (y)   ; GREPFIELD(static/info, '3        Na', 4) ; -1.046751
match ; Force 3 (z)   ; GREPFIELD(static/info, '3        Na', 5) ; -3.331477

match ; Partial charge  1 ; GREPFIELD(static/info, 'Partial ionic charges', 3, 2)  ; 1.0
match ; Partial charge  2 ; GREPFIELD(static/info, 'Partial ionic charges', 3, 3)  ; 1.0
match ; Density value 1   ; LINEFIELD(static/density.y=0\,z=0,  2, 2)   ; 5.84567251754064E-002
match ; Density value 2   ; LINEFIELD(static/density.y=0\,z=0,  3, 2)   ; 5.75863742617480E-002
match ; Bader value 1     ; LINEFIELD(static/bader-sp1.y=0\,z=0,  3, 2) ;-4.82188686242972E-002
match ; Bader value 2     ; LINEFIELD(static/bader-sp1.y=0\,z=0,  8, 2) ;-1.82891664464866E-001

