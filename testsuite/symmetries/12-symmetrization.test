# -*- coding: utf-8 mode: shell-script -*-

Test       : Real space symmetrization 
Program    : octopus
TestGroups : long-run, periodic_systems, symmetries
Enabled    : Yes

Input : 12-symmetrization.01-spg195_nosym.inp

match ; Total energy       ; GREPFIELD(static/info, 'Total       =', 3) ;  16.94225921
match ; Ion-ion energy     ; GREPFIELD(static/info, 'Ion-ion     =', 3) ;  13.99905464
match ; Eigenvalues sum    ; GREPFIELD(static/info, 'Eigenvalues =', 3) ;  2.47979940
match ; Hartree energy     ; GREPFIELD(static/info, 'Hartree     =', 3) ;  0.54032874
match ; Exchange energy    ; GREPFIELD(static/info, 'Exchange    =', 3) ; -2.77046391
match ; Correlation energy ; GREPFIELD(static/info, 'Correlation =', 3) ; -0.53914494
match ; Kinetic energy     ; GREPFIELD(static/info, 'Kinetic     =', 3) ;  3.96754682
match ; External energy    ; GREPFIELD(static/info, 'External    =', 3) ;  1.74487323

match ; Eigenvalue [  k=1, n=1   ] ; GREPFIELD(static/info, '#k =   1', 3, 1) ; -0.015068
match ; Eigenvalue [  k=1, n=2   ] ; GREPFIELD(static/info, '#k =   1', 3, 2) ;  0.222580
match ; Eigenvalue [  k=1, n=3   ] ; GREPFIELD(static/info, '#k =   1', 3, 3) ;  0.222599
match ; Eigenvalue [  k=1, n=4   ] ; GREPFIELD(static/info, '#k =   1', 3, 4) ;  0.274516
match ; Eigenvalue [  k=1, n=5   ] ; GREPFIELD(static/info, '#k =   1', 3, 5) ;  0.274556
match ; Eigenvalue [  k=1, n=6   ] ; GREPFIELD(static/info, '#k =   1', 3, 6) ;  0.274560

match ; Force 1 (x)   ; GREPFIELD(static/info, '1        Na', 3) ;  3.238067
match ; Force 1 (y)   ; GREPFIELD(static/info, '1        Na', 4) ;  1.048184
match ; Force 1 (z)   ; GREPFIELD(static/info, '1        Na', 5) ; -3.332886
match ; Force 2 (x)   ; GREPFIELD(static/info, '2        Na', 3) ; -3.330566
match ; Force 2 (y)   ; GREPFIELD(static/info, '2        Na', 4) ;  3.237912
match ; Force 2 (z)   ; GREPFIELD(static/info, '2        Na', 5) ;  1.045982
match ; Force 3 (x)   ; GREPFIELD(static/info, '3        Na', 3) ; -3.238064
match ; Force 3 (y)   ; GREPFIELD(static/info, '3        Na', 4) ; -1.048187
match ; Force 3 (z)   ; GREPFIELD(static/info, '3        Na', 5) ; -3.332890

match ; Partial charge  1 ; GREPFIELD(static/info, 'Partial ionic charges', 3, 2)  ; 1.0
match ; Partial charge  2 ; GREPFIELD(static/info, 'Partial ionic charges', 3, 3)  ; 1.0
match ; Density value 1   ; LINEFIELD(static/density.y=0\,z=0,  2, 2)   ; 5.84558003252311E-002
match ; Density value 2   ; LINEFIELD(static/density.y=0\,z=0,  3, 2)   ; 5.75819672330234E-002
match ; Bader value 1     ; LINEFIELD(static/bader-sp1.y=0\,z=0,  3, 2) ;-1.42081544438574E-001
match ; Bader value 2     ; LINEFIELD(static/bader-sp1.y=0\,z=0,  8, 2) ;-1.82913926929619E-001

