# -*- coding: utf-8 mode: shell-script -*-

Test       : Real space symmetrization 
Program    : octopus
TestGroups : long-run, periodic_systems, symmetries
Enabled    : Yes

Input : 12-symmetrization.01-spg195_nosym.inp

match ; Total energy       ; GREPFIELD(static/info, 'Total       =', 3) ;  16.94217865
match ; Ion-ion energy     ; GREPFIELD(static/info, 'Ion-ion     =', 3) ;  13.99905464
match ; Eigenvalues sum    ; GREPFIELD(static/info, 'Eigenvalues =', 3) ;  2.47983250
match ; Hartree energy     ; GREPFIELD(static/info, 'Hartree     =', 3) ;  0.54046844
match ; Exchange energy    ; GREPFIELD(static/info, 'Exchange    =', 3) ; -2.77054126
match ; Correlation energy ; GREPFIELD(static/info, 'Correlation =', 3) ; -0.53915099
match ; Kinetic energy     ; GREPFIELD(static/info, 'Kinetic     =', 3) ;  3.96755756
match ; External energy    ; GREPFIELD(static/info, 'External    =', 3) ;  1.74485014

match ; Eigenvalue [  k=1, n=1   ] ; GREPFIELD(static/info, '#k =   1', 3, 1) ; -0.015066
