# -*- coding: utf-8 mode: shell-script -*-

Test       : Loewdin orthogonalization and ACBN0 functional
Program    : octopus
TestGroups : long-run, periodic_systems, lda_u
Enabled    : Yes

Processors : 3

Input      : 08-loewdin.01-Si.inp

match ; Total k-points   ; GREPFIELD(static/info, 'Total number of k-points', 6) ; 27

match ; Total energy       ; GREPFIELD(static/info, 'Total       =', 3) ; -7.66793717
match ; Ion-ion energy     ; GREPFIELD(static/info, 'Ion-ion     =', 3) ; -7.85780080 
match ; Eigenvalues sum    ; GREPFIELD(static/info, 'Eigenvalues =', 3) ; -0.33965031
match ; Hartree energy     ; GREPFIELD(static/info, 'Hartree     =', 3) ;  0.55817715
match ; Exchange energy    ; GREPFIELD(static/info, 'Exchange    =', 3) ; -2.03733000
match ; Correlation energy ; GREPFIELD(static/info, 'Correlation =', 3) ; -0.37525194
match ; Kinetic energy     ; GREPFIELD(static/info, 'Kinetic     =', 3) ;  3.10160393
match ; External energy    ; GREPFIELD(static/info, 'External    =', 3) ; -1.29430669
match ; Hubbard energy     ; GREPFIELD(static/info, 'Hubbard     =', 3) ;  0.23692555

match ; k-point 1 (x) ; GREPFIELD(static/info, '#k =   1', 7) ; 0.000000 
match ; k-point 1 (y) ; GREPFIELD(static/info, '#k =   1', 8) ; 0.000000
match ; k-point 1 (z) ; GREPFIELD(static/info, '#k =   1', 9) ; 0.000000
match ; Eigenvalue  1 ; GREPFIELD(static/info, '#k =   1', 3, 1) ;  -0.285395
match ; Eigenvalue  2 ; GREPFIELD(static/info, '#k =   1', 3, 2) ;   0.141747
match ; Eigenvalue  3 ; GREPFIELD(static/info, '#k =   1', 3, 3) ;   0.141747
match ; Eigenvalue  4 ; GREPFIELD(static/info, '#k =   1', 3, 4) ;   0.141748

match ; U 3p Si1  ; LINEFIELD(static/effectiveU, 3, 4) ; 0.164430
match ; U 3p Si2  ; LINEFIELD(static/effectiveU, 4, 4) ; 0.164430


match ; Occupation Ni2 up-down 3d4 ; LINEFIELD(static/occ_matrices, -2, 3) ; 1.19853589
match ; Occupation Ni2 up-down 3d5 ; LINEFIELD(static/occ_matrices, -1, 5) ; 1.19853563

