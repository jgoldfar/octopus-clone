# -*- coding: utf-8 mode: shell-script -*-

Test       : ACBN0 functional for (AF) NiO and LiF bulk crystals
Program    : octopus
TestGroups : long-run, periodic_systems, lda_u
Enabled    : Yes


Input      : 02-ACBN0.01-nio.inp

Precision : 0.00015

match ; SCF convergence ; GREPCOUNT(static/info, 'SCF converged') ; 1

match ; Total k-points   ; GREPFIELD(static/info, 'Total number of k-points', 6) ; 4

match ; Total energy       ; GREPFIELD(static/info, 'Total       =', 3) ;  -286.41433675
match ; Ion-ion energy     ; GREPFIELD(static/info, 'Ion-ion     =', 3) ;  -177.00987193 
match ; Eigenvalues sum    ; GREPFIELD(static/info, 'Eigenvalues =', 3) ;  -35.35503355
match ; Hartree energy     ; GREPFIELD(static/info, 'Hartree     =', 3) ;   87.02411633 
match ; Exchange energy    ; GREPFIELD(static/info, 'Exchange    =', 3) ;  -33.72572561 
match ; Correlation energy ; GREPFIELD(static/info, 'Correlation =', 3) ;  -2.00732363
match ; Kinetic energy     ; GREPFIELD(static/info, 'Kinetic     =', 3) ;   186.49832794
match ; External energy    ; GREPFIELD(static/info, 'External    =', 3) ;  -347.33743149
match ; Hubbard energy     ; GREPFIELD(static/info, 'Hubbard     =', 3) ;   0.14356965
match ; Total Magnetic Moment ; GREPFIELD(static/info, 'mz = ', 3);  0.000000    
match ; Local Magnetic Moment (Ni1) ; GREPFIELD(static/info, '1        Ni', 3);  1.860381
match ; Local Magnetic Moment (Ni2) ; GREPFIELD(static/info, '2        Ni', 3); -1.860381
match ; Local Magnetic Moment (O1) ;  GREPFIELD(static/info, '3         O', 3);  0.0000
match ; Local Magnetic Moment (O2) ;  GREPFIELD(static/info, '4         O', 3);  0.0000

match ; k-point 1 (x) ; GREPFIELD(static/info, '#k =   1', 7) ; 0.000000 
match ; k-point 1 (y) ; GREPFIELD(static/info, '#k =   1', 8) ; 0.000000
match ; k-point 1 (z) ; GREPFIELD(static/info, '#k =   1', 9) ; 0.000000
match ; Eigenvalue  1 ; GREPFIELD(static/info, '#k =   1', 3, 1) ;  -1.993681
match ; Eigenvalue  8 ; GREPFIELD(static/info, '#k =   1', 3, 8) ;  -1.886550 
match ; Eigenvalue 16 ; GREPFIELD(static/info, '#k =   1', 3, 16) ; -0.791730
match ; Eigenvalue 17 ; GREPFIELD(static/info, '#k =   1', 3, 17) ; -0.790896

match ; Occupation Ni2 down 3d4 ; LINEFIELD(static/occ_matrices, -2, 7) ; 0.97566546
match ; Occupation Ni2 down 3d5 ; LINEFIELD(static/occ_matrices, -1, 9) ; 0.99465482

match ; U3d Ni1 ; LINEFIELD(static/effectiveU, -9, 4) ; 0.537011
match ; U3d Ni2 ; LINEFIELD(static/effectiveU, -9, 4) ; 0.537011 


Processors : 4
Precision : 0.0001

Input      : 02-ACBN0.02-lif.inp

match ; SCF convergence ; GREPCOUNT(static/info, 'SCF converged') ; 1
match ; Total energy       ; GREPFIELD(static/info, 'Total       =', 3) ;  -24.29880535
match ; Ion-ion energy     ; GREPFIELD(static/info, 'Ion-ion     =', 3) ;  -14.42250723
match ; Eigenvalues sum    ; GREPFIELD(static/info, 'Eigenvalues =', 3) ;  -4.44828873
match ; Hartree energy     ; GREPFIELD(static/info, 'Hartree     =', 3) ;   8.25856937
match ; Exchange energy    ; GREPFIELD(static/info, 'Exchange    =', 3) ;  -4.37464487
match ; Correlation energy ; GREPFIELD(static/info, 'Correlation =', 3) ;  -0.49320257
match ; Kinetic energy     ; GREPFIELD(static/info, 'Kinetic     =', 3) ;  17.56113759
match ; External energy    ; GREPFIELD(static/info, 'External    =', 3) ;  -30.88121168
match ; Hubbard energy     ; GREPFIELD(static/info, 'Hubbard     =', 3) ;   0.05302112
match ; Total Magnetic Moment ; GREPFIELD(static/info, 'mz = ', 3);  0.000000
match ; Local Magnetic Moment (Li) ; GREPFIELD(static/info, '1        Li', 3);  0.000000
match ; Local Magnetic Moment (F)  ; GREPFIELD(static/info, '2         F', 3);  0.000000

match ; k-point 2 (x) ; GREPFIELD(static/info, '#k =   2', 7) ;  0.000000
match ; k-point 2 (y) ; GREPFIELD(static/info, '#k =   2', 8) ;  0.500000
match ; k-point 2 (z) ; GREPFIELD(static/info, '#k =   2', 9) ;  0.000000
match ; Eigenvalue  1 up ; GREPFIELD(static/info, '#k =   2', 3, 1) ;  -0.943600
match ; Eigenvalue  1 dn ; GREPFIELD(static/info, '#k =   2', 3, 2) ;  -0.943600
match ; Eigenvalue  3 up ; GREPFIELD(static/info, '#k =   2', 3, 5) ;  -0.401387
match ; Eigenvalue  4 up ; GREPFIELD(static/info, '#k =   2', 3, 7) ;  -0.400786
match ; Eigenvalue  5 up ; GREPFIELD(static/info, '#k =   2', 3, 9) ;   0.283912

match ; Occupation F down 2p2 ; LINEFIELD(static/occ_matrices, -2, 3) ; 0.96115135
match ; Occupation F down 2p3 ; LINEFIELD(static/occ_matrices, -1, 5) ; 0.96115135

match ; U2p F   ; LINEFIELD(static/effectiveU, -7, 4) ; 0.473318
