# -*- coding: utf-8 mode: shell-script -*-

Test       : GGA+U for (AF) bulk NiO crystal with noncollinear spins
Program    : octopus
TestGroups : long-run, periodic_systems, lda_u
Enabled    : Yes

Processors : 4

Input      : 07-noncollinear.01-U5-gs.inp

match ; Total k-points   ; GREPFIELD(static/info, 'Total number of k-points', 6) ; 8

match ; Total energy       ; GREPFIELD(static/info, 'Total       =', 3) ; -246.82740045
match ; Ion-ion energy     ; GREPFIELD(static/info, 'Ion-ion     =', 3) ; -177.00987193 
match ; Eigenvalues sum    ; GREPFIELD(static/info, 'Eigenvalues =', 3) ; -32.63669954
match ; Hartree energy     ; GREPFIELD(static/info, 'Hartree     =', 3) ;  45.62760349 
match ; Exchange energy    ; GREPFIELD(static/info, 'Exchange    =', 3) ; -27.51808195 
match ; Correlation energy ; GREPFIELD(static/info, 'Correlation =', 3) ; -2.04230470
match ; Kinetic energy     ; GREPFIELD(static/info, 'Kinetic     =', 3) ;  122.05225252 
match ; External energy    ; GREPFIELD(static/info, 'External    =', 3) ; -208.20893366 
match ; Hubbard energy     ; GREPFIELD(static/info, 'Hubbard     =', 3) ;  0.26999500
match ; Fermi energy       ; GREPFIELD(static/info, 'Fermi energy =', 4);  0.414935
match ; Total Magnetic Moment ; GREPFIELD(static/info, 'mx = ', 3);      0.0000 
match ; Local Magnetic Moment (Ni1) ; GREPFIELD(static/info, '1        Ni', 3);  3.274876 
match ; Local Magnetic Moment (Ni2) ; GREPFIELD(static/info, '2        Ni', 3); -3.273884
match ; Local Magnetic Moment (O1) ;  GREPFIELD(static/info, '3         O', 3);  0.0000 
match ; Local Magnetic Moment (O2) ;  GREPFIELD(static/info, '4         O', 3);  0.0000 

match ; k-point 1 (x) ; GREPFIELD(static/info, '#k =   1', 7) ; 0.000000 
match ; k-point 1 (y) ; GREPFIELD(static/info, '#k =   1', 8) ; 0.000000
match ; k-point 1 (z) ; GREPFIELD(static/info, '#k =   1', 9) ; 0.000000
match ; Eigenvalue  1 ; GREPFIELD(static/info, '#k =   1', 3, 1) ;  -3.076655
match ; Eigenvalue  8 ; GREPFIELD(static/info, '#k =   1', 3, 8) ;  -2.918580 
match ; Eigenvalue 16 ; GREPFIELD(static/info, '#k =   1', 3, 16) ; -0.430454
match ; Eigenvalue 17 ; GREPFIELD(static/info, '#k =   1', 3, 17) ; -0.166458 

match ; Occupation Ni2 up-down 3d4 ; LINEFIELD(static/occ_matrices, -2, 7) ; -0.26913166
match ; Occupation Ni2 up-down 3d5 ; LINEFIELD(static/occ_matrices, -1, 9) ; -0.37794672

