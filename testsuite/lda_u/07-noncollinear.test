# -*- coding: utf-8 mode: shell-script -*-

Test       : GGA+U for (AF) bulk NiO crystal with noncollinear spins
Program    : octopus
TestGroups : long-run, periodic_systems, lda_u
Enabled    : Yes

Input      : 07-noncollinear.01-U5-gs.inp

match ; Total k-points   ; GREPFIELD(static/info, 'Total number of k-points', 6) ; 8

match ; Total energy       ; GREPFIELD(static/info, 'Total       =', 3) ; -246.82817544
match ; Ion-ion energy     ; GREPFIELD(static/info, 'Ion-ion     =', 3) ; -177.00987193 
match ; Eigenvalues sum    ; GREPFIELD(static/info, 'Eigenvalues =', 3) ; -32.63789552
match ; Hartree energy     ; GREPFIELD(static/info, 'Hartree     =', 3) ;  45.62749301 
match ; Exchange energy    ; GREPFIELD(static/info, 'Exchange    =', 3) ; -27.51836500 
match ; Correlation energy ; GREPFIELD(static/info, 'Correlation =', 3) ; -2.04225239
match ; Kinetic energy     ; GREPFIELD(static/info, 'Kinetic     =', 3) ;  122.05238948 
match ; External energy    ; GREPFIELD(static/info, 'External    =', 3) ; -208.20859399 
match ; Hubbard energy     ; GREPFIELD(static/info, 'Hubbard     =', 3) ;  0.26976808
match ; Fermi energy       ; GREPFIELD(static/info, 'Fermi energy =', 4);  0.414984
match ; Total Magnetic Moment ; GREPFIELD(static/info, 'mx = ', 3);      0.0000 
match ; Local Magnetic Moment (Ni1) ; GREPFIELD(static/info, '1        Ni', 3);  3.274874 
match ; Local Magnetic Moment (Ni2) ; GREPFIELD(static/info, '2        Ni', 3); -3.274755
match ; Local Magnetic Moment (O1) ;  GREPFIELD(static/info, '3         O', 3);  0.0000 
match ; Local Magnetic Moment (O2) ;  GREPFIELD(static/info, '4         O', 3);  0.0000 

match ; k-point 1 (x) ; GREPFIELD(static/info, '#k =   1', 7) ; 0.000000 
match ; k-point 1 (y) ; GREPFIELD(static/info, '#k =   1', 8) ; 0.000000
match ; k-point 1 (z) ; GREPFIELD(static/info, '#k =   1', 9) ; 0.000000
match ; Eigenvalue  1 ; GREPFIELD(static/info, '#k =   1', 3, 1) ;  -3.076965
match ; Eigenvalue  8 ; GREPFIELD(static/info, '#k =   1', 3, 8) ;  -2.918719 
match ; Eigenvalue 16 ; GREPFIELD(static/info, '#k =   1', 3, 16) ; -0.430251
match ; Eigenvalue 17 ; GREPFIELD(static/info, '#k =   1', 3, 17) ; -0.166704 

match ; Occupation Ni2 up-down 3d4 ; LINEFIELD(static/occ_matrices, -2, 7) ; 0.01455103
match ; Occupation Ni2 up-down 3d5 ; LINEFIELD(static/occ_matrices, -1, 9) ; -0.37797327

