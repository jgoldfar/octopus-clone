# -*- coding: utf-8 mode: shell-script -*-

Test       : Hubbard forces
Program    : octopus
TestGroups : short-run, finite_systems_3d, lda_u
Enabled    : Yes

Input      : 05-forces.01-Na2.inp

match ; Total energy       ; GREPFIELD(static/info, 'Total       =', 3) ;  -0.35835573
match ; Ion-ion energy     ; GREPFIELD(static/info, 'Ion-ion     =', 3) ;   0.16666667
match ; Eigenvalues sum    ; GREPFIELD(static/info, 'Eigenvalues =', 3) ;  -0.16059110
match ; Hartree energy     ; GREPFIELD(static/info, 'Hartree     =', 3) ;   0.46299738
match ; Exchange energy    ; GREPFIELD(static/info, 'Exchange    =', 3) ;  -0.20315950
match ; Correlation energy ; GREPFIELD(static/info, 'Correlation =', 3) ;  -0.05902256
match ; Kinetic energy     ; GREPFIELD(static/info, 'Kinetic     =', 3) ;   0.18345229
match ; External energy    ; GREPFIELD(static/info, 'External    =', 3) ;  -0.94351049
match ; Hubbard energy     ; GREPFIELD(static/info, 'Hubbard     =', 3) ;   0.03421882

match ; Eigenvalue  1 ; GREPFIELD(static/info, '1   --', 3) ;  -0.080296

match ; Max abs force ; GREPFIELD(static/info, 'Max abs force', 6) ; 0.006203
match ;   Total force ; GREPFIELD(static/info, 'Total force', 5)   ; 0.000000
