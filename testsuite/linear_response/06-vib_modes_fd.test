# -*- coding: utf-8 mode: shell-script -*-
# $Id$

Test       : LiH vibrational frequency finite differences
Program    : octopus
TestGroups : long-run, linear_response
Enabled    : Yes

Processors : 4

Input: 06-vib_modes_fd.01-ground_state.inp
match ; Total Energy   ; GREPFIELD(static/info, 'Total       =', 3) ; -0.76459868; 0.0001
match ; Eigenvalues    ; GREPFIELD(static/info, '1   --', 3) ; -0.141240; 0.0001
match ; Dipole Moment  ; GREPFIELD(static/info, '<z>', 3) ; 2.24560; 0.00011

Input: 06-vib_modes_fd.01-vib_modes_fd.inp
match ; Vibrational Frequency   ; LINEFIELD(vib_modes/normal_frequencies_fd, 1, 2) ; 1421.73309602; 0.002
match ; Dynamical Matrix Entry  ; LINEFIELD(vib_modes/dynamical_matrix_fd, 2, 5)  ; -0.0182025051759; 1.8e-06
