# -*- coding: utf-8 mode: shell-script -*-
# $Id$

Test       : Photoelectron linear regime (flux 3D)
Program    : octopus
TestGroups : long-run, photo_electron
Enabled    : Yes


# Na groundstate
Processors : 2
Input      : 09-flux_3d.01-Na_gs.inp

# Na photoemission
# Processors : 2
Precision  : 1.0e-12
Input      : 09-flux_3d.02-Na_td.inp
match ; PES [val  1] ; LINEFIELD(td.general/PES_flux.power.sum, 136, 2) ; 0.1962140759E-012
match ; PES [val  2] ; LINEFIELD(td.general/PES_flux.power.sum, 141, 2) ; 0.2200683471E-010
match ; PES [val  3] ; LINEFIELD(td.general/PES_flux.power.sum, 144, 2) ; 0.9374485331E-011

