# -*- coding: utf-8 mode: shell-script -*-
# $Id$

Test       : Photoelectron linear regime (spm 3D)
Program    : octopus
TestGroups : long-run, photo_electron
Enabled    : Yes


# Na groundstate
Processors : 2
Input      : 12-spm_3d.01-Na_gs.inp

# Na photoemission
# Processors : 2
Precision  : 1.0e-11
Input      : 12-spm_3d.02-Na_td.inp
match ; PES [val  1] ; LINEFIELD(td.general/PES_spm.power.sum, 40, 2) ; 0.1133466030E-008
match ; PES [val  2] ; LINEFIELD(td.general/PES_spm.power.sum, 41, 2) ; 0.1579822004E-008
match ; PES [val  3] ; LINEFIELD(td.general/PES_spm.power.sum, 42, 2) ; 0.1101871196E-008

