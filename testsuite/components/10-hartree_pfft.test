# -*- coding: utf-8 mode: shell-script -*-
# $Id$

Test       : Hartree pfft
Options    : pfft
Program    : oct-test
TestGroups : short-run, components
Enabled    : Yes

Input: 10-hartree_pfft.01-fft.inp
match ; FFT           ; GREPFIELD(hartree_results, 'Hartree test (abs.) =', 5) ; 0.00035839; 0.005

Input: 10-hartree_pfft.02-fft_corrected.inp
match ; FFT Corrected ; GREPFIELD(hartree_results, 'Hartree test (abs.) =', 5) ; 0.00154839; 0.005

