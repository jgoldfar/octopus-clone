# -*- coding: utf-8 mode: shell-script -*-
# $Id$

Test       : Modified Gram-Schmidt orthogonalization
Program    : oct-test
TestGroups : long-run, components
Enabled    : Yes

Precision : 1.0e-13
Input: 08-mgs.01-real.inp
match ; Overlap 1 1 ; GREPFIELD(out, '      1      1', 3) ; 0.100000E+01
match ; Overlap 1 2 ; GREPFIELD(out, '      1      2', 3) ; 0.767563E-16
match ; Overlap 1 9 ; GREPFIELD(out, '      1      9', 3) ; 0.600960E-15
match ; Overlap 6 6 ; GREPFIELD(out, '      6      6', 3) ; 0.100000E+01
match ; Overlap 7 8 ; GREPFIELD(out, '      7      8', 3) ; 0.681882E-15
match ; Overlap 9 9 ; GREPFIELD(out, '      9      9', 3) ; 0.100000E+01

Input: 08-mgs.02-complex.inp
match ; Overlap 1 1 ; GREPFIELD(out, '      1      1', 3) ; 0.100000E+01
match ; Overlap 1 2 ; GREPFIELD(out, '      1      2', 3) ; 0.193433E-15
match ; Overlap 1 9 ; GREPFIELD(out, '      1      9', 3) ; 0.879240E-16
match ; Overlap 6 6 ; GREPFIELD(out, '      6      6', 3) ; 0.100000E+01
match ; Overlap 7 8 ; GREPFIELD(out, '      7      8', 3) ; 0.112430E-13
match ; Overlap 9 9 ; GREPFIELD(out, '      9      9', 3) ; 0.100000E+01
